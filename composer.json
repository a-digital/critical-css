{
    "name": "adigital/critical-css",
    "description": "Generate critical css without the need for ssh access",
    "type": "craft-plugin",
    "version": "2.0.0",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "critical css"
    ],
    "support": {
        "docs": "https://github.com/a-digital/critical-css/blob/master/README.md",
        "issues": "https://github.com/a-digital/critical-css/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "A Digital",
            "homepage": "https://adigital.agency"
        }
    ],
    "require": {
        "craftcms/cms": "^4.0.0"
    },
    "autoload": {
        "psr-4": {
          "adigital\\criticalcss\\": "src/"
        }
    },
    "extra": {
        "name": "Critical CSS",
        "handle": "critical-css",
        "hasCpSettings": true,
        "hasCpSection": true,
        "changelogUrl": "https://raw.githubusercontent.com/a-digital/critical-css/master/CHANGELOG.md",
        "components": {
            "criticalCssService": "adigital\\criticalcss\\services\\CriticalCssService"
        },
        "class": "adigital\\criticalcss\\CriticalCss"
    }
}
